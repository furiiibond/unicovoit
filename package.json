        "@nuxtjs/firebase": "^8.2.2",
        "@nuxtjs/i18n": "^7.3.0",
        "@nuxtjs/pwa": "^3.3.5",
        "axios": "^0.27.2",
        "cookie-parser": "^1.4.6",
        "cookie-universal-nuxt": "^2.2.2",
        "core-js": "^3.26.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-fileupload-validator": "^2.0.1",
        "express-oauth2-jwt-bearer": "^1.3.0",
        "express-rate-limit": "^6.7.0",
        "fast-xml-parser": "^4.0.13",
        "firebase": "^9.18.0",
        "helmet": "^6.0.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^6.9.2",
        "nodemailer": "^6.8.0",
