        "vue-server-renderer": "^2.6.14",
        "vue-template-compiler": "^2.7.10",
        "vuetify": "^2.6.14",
        "vuetify-loader": "^1.9.2",
        "webpack": "^4.46.0",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@capacitor/android": "^4.3.0",
        "@capacitor/cli": "^4.3.0",
        "@capacitor/core": "^4.3.0",
